<?xml version="1.0" encoding="utf-8"?>
<!-- ===============================================================

This document is the default setting of camera for Neutral environment.

attribute description
enable - 1:on; 0:off
extend - 1:yes; 0:no => extend Default Model
reboot - 1:yes; 0:no

event pattern :
mp4,%d,%d,1 : 8026, 8261, 8321, 8110
mp4,2,%d,%d : 8221, 821,
==================================================================== -->
<Cameras xmlns="http://www.sercomm.com/camera" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sercomm.com/camera camera.xsd ">
	<!-- Default setting ################################################################################################### -->
	<Model type="DEFAULT">
		<Audio>
			<Group name="AUDIO" extend="0">
				<AudioIn text="audio_in" enable="1">
					<prop key="in_audio_type">5</prop>
					<prop key="in_audio_type2">3</prop>
					<prop key="in_audio_type3">1</prop>
				</AudioIn>
				<AudioOut text="audio_out" enable="0">
				</AudioOut>
			</Group>
		</Audio>
		<Stream>
			<Group name="JPEG" extend="0">
				<mode text="mode" enable="1">
					<prop key="resolution">2</prop>
					<prop key="quality_level">3</prop>
					<prop key="frame_rate">5</prop>
				</mode>
				<mode text="mode2" enable="0"/>
				<mode text="mode3" enable="0"/>
			</Group>
			<Group name="H264" extend="0">
				<mode text="mode" enable="0"/>
				<mode text="mode2" enable="1">
					<prop key="resolution2">2</prop>
					<prop key="quality_type2">1</prop>
					<prop key="quality_level2">3</prop>
					<prop key="bit_rate2">128</prop>
					<prop key="gov_length2">30</prop>
					<prop key="frame_rate2">25</prop>
				</mode>
				<mode text="mode3" enable="0"/>
			</Group>
			<Group name="MPEG4" extend="0">
				<mode text="mode" enable="0"/>
				<mode text="mode2" enable="0"/>
				<mode text="mode3" enable="1">
					<prop key="resolution3">2</prop>
					<prop key="quality_type3">1</prop>
					<prop key="quality_level3">3</prop>
					<prop key="bit_rate3">1</prop>
					<prop key="gov_length3">30</prop>
					<prop key="frame_rate3">25</prop>
				</mode>
			</Group>
		</Stream>
		<Other>
			<Group name="HTTP_EVENT" text="http_event_en" enable="1" extend="0" reboot="0">
				<prop key="http_post_en">1</prop>
				<prop key="http_post_url">http://timvss-cms-collaudo.sunto.reply.it:8080/storage/set_media</prop><!-- CMS_META -->
			</Group>
			<Group name="EVENT" text="event_trigger" enable="1" extend="0" reboot="0"><!-- default preCapture and postCapture -->
				<prop key="event_trigger">1</prop>
				<prop key="event1_entry">is=2|es=0,|et=5|acts=op1:0;op2:0;email:0;ftpu:0;im:0;httpn:0;httppost:1;wlled:0;smbc:0;sd:0;op3:0;op4:0;smbc_rec:0;sd_rec:0|ei=0|ea=mp4,2,3,1|en=HTTP_POST</prop>
			</Group>
			<Group name="HTTP_NOTIFY" text="http_notify" enable="0" extend="0" reboot="0">
				<!--  !!! DO NOT DELETE !!! - for reference
				<prop key="http_url"></prop>
				<prop key="http_proxy"></prop>
				<prop key="http_proxy_no">80</prop>
				<prop key="http_method"></prop>
				<prop key="http_user"></prop>
				<prop key="http_password"></prop>
				<prop key="proxy_user"></prop>
				<prop key="proxy_password"></prop>
				<prop key="event_data_flag"></prop>
				-->
			</Group>
			<Group name="SYSTEM" text="ntp_mode" enable="1" extend="0">
				<prop key="ntp_server">ntp1.sunto.reply.it</prop>
				<prop key="time_zone">27</prop>
				<!-- <prop key="daylight_saving">0</prop> -->
				<!-- <prop key="ntp_server2">ntp2.sunto.reply.it</prop> -->
			</Group>
		</Other>
	</Model>

