<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <!-- Relay Server Configuration -->
    <entry key="SERV.APACHE.PROTOCOL">http</entry>
    <entry key="SERV.APACHE.HTTPPORT">8080</entry>
    <entry key="SERV.APACHE.HTTPSPORT">8443</entry>
    <entry key="SERV.APACHE.MAX_THREAD">500</entry>
    <entry key="SERV.APACHE.ACCEPT_COUNT">250</entry>    
    <entry key="SERV.KEY_STORE.PASSWORD">zaq12wsx</entry>
    <entry key="SERV.LIVEVIEW.TIMEOUT">120000</entry>
    <entry key="SERV.CLIPFILE_UPLOAD.TIMEOUT">150000</entry>
    
    <!-- The number is MAX thread which DAEMON and Service can consume -->
    <entry key="SERV.DAEMON.MAX_THREAD">256</entry>
    
    <!-- mutible(two-way) authenticate -->
    <entry key="SERV.APACHE.MUTIBLE.AUTH">0</entry><!-- 0:disable, 1:enable -->
    <entry key="SERV.APACHE.MUTIBLE.HTTPSPORT">8444</entry>
    <entry key="SERV.TRUST_STORE.PASSWORD">zaq12wsx</entry>

    <!-- NFS information -->
    <entry key ="NFS.MOUNT.PATH">/var/media</entry>
    
    <!-- White list of proxy location to allow to pass invoking BMSi -->
    <entry key ="BMS.LOCATION.CHECK">N</entry>
    <entry key ="BMS.LOCATION">neutral_bms</entry>

    <!-- White list of SL location to allow to pass invoking CMSi(/cgi/*) -->
    <entry key ="SL.LOCATION.CHECK">N</entry>
    <entry key ="SL.LOCATION">neutral_sl</entry>

    <!-- Reduce size of clip file size to ? percentage, the range is 50 ~ 100 -->
    <entry key="REDUCE.CLIP.FILE">100</entry>
    
    <!-- CMS CORE information -->
    <entry key="CMS_CORE.ADDR">neutral_dl</entry>
    <entry key="CMS_CORE.PORT">8081</entry>
    <entry key="CMS_CORE.PRC">8090</entry>
    <entry key="CMS_CORE.NUMBER">2</entry>
    
    <entry key="NEW.CMS_CORE">N</entry>
    <entry key="NEW.CMS_CORE.ADDR">neutral_dl</entry>
    <entry key="NEW.CMS_CORE.PORT">8081</entry>
    <entry key="NEW.CMS_CORE.PRC">8090</entry>
    <entry key="NEW.CMS_CORE.NUMBER">2</entry>
     
    <entry key="CHANGE.WORKER.FW.VER">3.0.35R05</entry>
</properties>
